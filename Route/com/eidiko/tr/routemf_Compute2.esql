BROKER SCHEMA com.eidiko.tr


CREATE COMPUTE MODULE routemf_Compute2
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		set OutputRoot.XMLNSC.status='Testing';
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyMessageHeaders() BEGIN
		DECLARE I INTEGER 1;
		DECLARE J INTEGER;
		SET J = CARDINALITY(InputRoot.*[]);
		WHILE I < J DO
			SET OutputRoot.*[I] = InputRoot.*[I];
			SET I = I + 1;
		END WHILE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputRoot = InputRoot;
	END;
END MODULE;
